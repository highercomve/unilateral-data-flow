  "description": "Testing redux",
  "main": "index.js",
  "scripts": {
    "watch": "watchify src/app.js -d -o app/js/app.js -v",
    "watch:test": "watchify test/index.js -d -o app/js/test.js -v",
    "build": "browserify src/app.js -d -v | uglifyjs -c > app/js/app.js",
    "build:test": "browserify test/index.js -d -v | uglifyjs -c > app/js/test.js",
    "start": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
